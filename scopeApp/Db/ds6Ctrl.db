record( mbbo, "$(P):MBBO_TRIG_SOU"){
  field( DESC, "Trig Source:")
  field( NOBT, "8")
  field( ZRVL, "0x0")
  field( ONVL, "0x1")
  field( TWVL, "0x2")
  field( THVL, "0x3")
  field( FRVL, "0x4")
  field( FVVL, "0x5")
  field( SXVL, "0x6")
  field( ZRST, "Chan1")
  field( ONST, "Chan2")
  field( TWST, "Chan3")
  field( THST, "Chan4")
  field( FRST, "EXT")
  field( FVST, "EXT5")
  field( SXST, "Line")
  field( DTYP, "asynInt32")
  field( OUT,  "@asyn($(PORT),$(N),1)MBBO_TRSOU")
  info( asyn:READBACK, "1")
}
