#---- $(DESC) -----------------------------------
record( mbbo, "$(P):MBBO_$(ITEM)"){
  field( DESC, "$(DESC):")
  field( NOBT, "13")
  field( ZRVL, "0x0")
  field( ONVL, "0x1")
  field( TWVL, "0x2")
  field( THVL, "0x3")
  field( FRVL, "0x4")
  field( FVVL, "0x5")
  field( SXVL, "0x6")
  field( SVVL, "0x7")
  field( EIVL, "0x8")
  field( NIVL, "0x9")
  field( TEVL, "0xa")
  field( ELVL, "0xb")
  field( TVVL, "0xc")
  field( ZRST, "1mV")
  field( ONST, "2mV")
  field( TWST, "5mV")
  field( THST, "10mV")
  field( FRST, "20mV")
  field( FVST, "50mV")
  field( SXST, "100mV")
  field( SVST, "200mV")
  field( EIST, "500mV")
  field( NIST, "1V")
  field( TEST, "2V")
  field( ELST, "5V")
  field( TVST, "10V")
  field( DTYP, "asynInt32")
  field( OUT,  "@asyn($(PORT),$(N),1)MBBO_$(USER)")
