  field( ONVL, "0x1")
  field( TWVL, "0x2")
  field( THVL, "0x3")
  field( FRVL, "0x4")
  field( FVVL, "0x5")
  field( SXVL, "0x6")
  field( SVVL, "0x7")
  field( EIVL, "0x8")
  field( ZRST, "1")
  field( ONST, "2")
  field( TWST, "4")
  field( THST, "10")
  field( FRST, "20")
  field( FVST, "40")
  field( SXST, "100")
  field( SVST, "200")
  field( EIST, "400")
  field( DTYP, "asynInt32")
  field( OUT,  "@asyn($(PORT),0,1)MBBO_TIMDIVV")
