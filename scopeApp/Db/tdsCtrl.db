record( mbbo, "$(P):MBBO:TIM:DIV:U"){
  field( DESC, "Time Division unit:")
  field( NOBT, "4")
  field( ZRVL, "0x0")
  field( ONVL, "0x1")
  field( TWVL, "0x2")
  field( THVL, "0x3")
  field( ZRST, "ns")
  field( ONST, "us")
  field( TWST, "ms")
  field( THST, "s")
  field( DTYP, "asynInt32")
  field( OUT,  "@asyn($(PORT),0,1)MBBO_TIMDIVU")
