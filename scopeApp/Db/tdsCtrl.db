record( mbbi, "$(P):MBBI:TRIG:STATE"){
  field( DESC, "Trigger State")
  field( SCAN, "I/O Intr")
  field( DTYP, "asynInt32")
  field( INP,  "@asyn($(PORT),0,1)MBBI_TRSTA")
  field( NOBT, "4")
  field( ZRVL, "0")
  field( ONVL, "1")
  field( TWVL, "2")
  field( THVL, "3")
  field( FRVL, "4")
  field( ZRST, "Auto")
  field( ONST, "Armed")
  field( TWST, "Ready")
  field( THST, "Save")
  field( FRST, "Trig'd")
}
record( mbbo, "$(P):MBBO:TRIG:SOU"){
  field( DESC, "Trig Source:")
  field( NOBT, "8")
  field( ZRVL, "0x0")
  field( ONVL, "0x1")
  field( TWVL, "0x2")
  field( THVL, "0x3")
  field( FRVL, "0x4")
  field( FVVL, "0x5")
  field( SXVL, "0x6")
  field( SVVL, "0x7")
  field( ZRST, "Chan1")
  field( ONST, "Chan2")
  field( TWST, "Chan3")
  field( THST, "Chan4")
  field( FRST, "LINE")
  field( FVST, "VERTICAL")
  field( SXST, "EXT10")
  field( SVST, "EXT")
  field( DTYP, "asynInt32")
  field( OUT,  "@asyn($(PORT),$(N),1)MBBO_TRSOU")
  info( asyn:READBACK, "1")
}
record( mbbo, "$(P):MBBO:TIM:DIV:V"){
