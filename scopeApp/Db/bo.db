
#---- $(DESC) -----------------------------------
record(bo, "$(P):BO_$(ITEM)"){
    field(DESC, "$(DESC)")
    field(DTYP, "asynInt32")
    field(OUT,  "@asyn($(PORT),$(N),1) BO_$(USER)")
    field(ZNAM, "$(ZNAM)")
    field(ONAM, "$(ONAM)")
    field(VAL,  "$(VAL=0)")
    field(SCAN, "$(SCAN=0)")
    field(PINI, "$(PINI=0)")
    info(asyn:READBACK, "1")
    info(autosaveFields, "$(AUTOSAVE=)")
}

